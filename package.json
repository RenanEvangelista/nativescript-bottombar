{
  "name": "nativescript-bottombar",
  "version": "3.0.0",
  "description": "NativeScript plugin for AHBottomNavigation.",
  "main": "bottombar",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "3.0.0",
      "ios": "3.0.0"
    }
  },
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo && cd demo && tns emulate ios",
    "demo.android": "npm run preparedemo && cd demo && tns run android",
    "preparedemo": "npm run build && cd demo && tns plugin remove nativescript-bottombar && tns plugin add .. && tns install",
    "setup": "cd demo && npm install && cd .. && npm run build && cd demo && tns plugin add .. && cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhanbIT/nativescript-bottombar.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "TypeScript",
    "Angular",
    "BottomBar",
    "Bottom bar",
    "Material Design",
    "bottom navigation",
    "tab navigation",
    "Android"
  ],
  "author": {
    "name": "rhanbIT",
    "email": "aniel.jeanbaptiste@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/rhanbIT/nativescript-bottombar/issues"
  },
  "homepage": "https://github.com/rhanbIT/nativescript-bottombar",
  "readmeFilename": "README.md",
  "devDependencies": {
    "typescript": "^2.2.2",
    "tns-core-modules": "^3.0.0 || ^3.0.0-rc.1",
    "tns-platform-declarations": "^3.0.0 || ^3.0.0-rc.1",
    "prompt": "^1.0.0",
    "rimraf": "^2.5.0"
  },
  "peerDependencies": {
    "tns-core-modules": "^3.0.0 || ^3.0.0-rc.1"
  }
}
